  "description": "Modern webpack 5 boilerplate",
  "version": "1.0.107",
  "private": true,
  "main": "src/index.tsx",
  "sideEffects": false,
  "dependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@mozaic-ds/react": "1.1.0",
    "@reduxjs/toolkit": "1.6.2",
    "axios": "0.24.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "compose-function": "3.0.3",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.1.4",
    "dotenv": "10.0.0",
    "history": "5.1.0",
    "html-webpack-plugin": "5.5.0",
    "i18next": "21.5.2",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-http-backend": "1.3.1",
    "json-minimizer-webpack-plugin": "3.1.1",
    "mini-css-extract-plugin": "2.4.4",
    "postcss": "8.3.11",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.0",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.14.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.0.1",
    "redux-saga": "1.1.3",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "webpack": "5.62.2",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.4.0",
    "webpack-merge": "5.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@types/compose-function": "0.0.30",
    "@types/jest": "27.0.2",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "autoprefixer": "9.8.8",
    "babel-jest": "27.3.1",
    "babel-preset-react": "6.24.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-react-redux": "3.3.2",
    "eslint-plugin-sonarjs": "0.10.0",
    "eslint-plugin-testing-library": "5.0.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "11.2.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "sonarqube-scanner": "2.8.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "docker:dev": "docker-compose --env-file .env -f docker/docker-compose.dev.yml up && docker-compose rm -fsv",
    "docker:run-prod": "docker run -p 80:80 --name webpack-boilerplate webpack-boilerplate-image-name && docker rm -fv webpack-boilerplate",
    "docker:build-prod": "docker-compose --env-file .env -f docker/docker-compose.prod.yml build",
    "ts:check": "tsc --project tsconfig.json --noEmit",
    "lint:fix": "eslint --fix --color \"**/*.{ts,tsx}\" src",
    "lint:check": "eslint --color \"**/*.{ts,tsx}\" src",
    "prettier:format": "prettier -w --loglevel silent './**/*.{ts,tsx,css,json}'",
    "fix": "npm-run-all prettier:format lint:fix",
    "test": "jest --coverage",
    "version:patch": "npm version patch --no-git-tag-version && git add package.json package-lock.json",
    "pre-commit": "npm-run-all ts:check prettier:format lint:check version:patch",
    "posttest": "sonar-scanner"
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albru/webpack-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albru/webpack-boilerplate"
  },
  "homepage": "https://github.com/albru/webpack-boilerplate#readme"
}
